	xmallocoverrun)	ac_xmalloc_overrun=1 ;;
	conslist)	ac_gc_check_cons_list=1 ;;
	glyphs)		ac_glyphs_debug=1 ;;
	*)	as_fn_error $? "unknown check category $check" "$LINENO" 5 ;;
	esac
done
